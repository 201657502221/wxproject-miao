.userInfo{
  height: 150rpx;display: flex;border-bottom: 1rpx #666 solid;align-items:center;
}
.userInfo image{
  width: 100rpx;height: 100rpx;border-radius: 50%;overflow: hidden;
}
.userInfo text{
  margin-left: 10rpx;
}
.userInfo button{
  width: 300rpx;margin-left: 20rpx;
}

.user-list{
  margin: 20rpx;
}
.user-list-item{
  display: flex;height: 50rpx;border-bottom: 1rpx #666 dashed;padding: 10rpx;
  align-items:center;justify-content: space-between;
}
